{
  "name": "eleventy-blog-starter",
  "version": "0.0.1",
  "description": "A starter template for blog website using Eleventy static site generator",
  "scripts": {
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "css": "postcss src/assets/css/style.css --o _site/assets/css/style.css --watch",
    "start": "npm-run-all --parallel css serve",
    "build": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/assets/css/style.css --o _site/assets/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/biswajit-saha/eleventy-blog-starter.git"
  },
  "author": {
    "name": "Biswajit Saha"
  },
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/biswajit-saha/eleventy-blog-starter#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.3.6",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "eleventy-plugin-excerpt": "^1.1.2",
    "eleventy-plugin-reading-time": "0.0.1",
    "eleventy-plugin-svg-sprite": "^1.2.1",
    "luxon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-cli": "^9.0.0",
    "tailwindcss": "^2.2.16"
  }
}
